      "package": "neighbournet.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "af74cc8a-a72e-4710-bd08-bedf7d94c6b3"
      }
    },
    "plugins": ["expo-router"]
  }
}
